<odoo>
    <record id="flat_list_action" model="ir.actions.act_window">
        <field name="name">Flats</field>
        <field name="res_model">realtor.flat</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="offer_list_action" model="ir.actions.act_window">
        <field name="name">Make Offer</field>
        <field name="res_model">realtor.offer</field>
        <field name="view_mode">tree,form</field>
    </record>
    <menuitem id="main_realtor_menu" name="Realtor">
        <menuitem id="realtor_menu" name="Options">
            <menuitem id="flat_menu" action="flat_list_action"/>  
            <menuitem id="offer_flat_menu" action="offer_list_action"/>  
        </menuitem>  
    </menuitem>
   
</odoo>